    "tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc-9 활성 파일 빌드",
			"command": "/usr/bin/gcc-9",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
